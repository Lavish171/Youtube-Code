import java.util.Scanner;
public class Main15 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        while (t-->0)
        {
            long vanilla=sc.nextLong();
            long choclate=sc.nextLong();
            long totalguest_firsttype=sc.nextLong();
            long totalguest_secondtype=sc.nextLong();
            if(vanilla +choclate < totalguest_firsttype+totalguest_secondtype) System.out.println("NO");
            else
            {
                long min=0;
                if(vanilla<=choclate) min=vanilla;
                else min=choclate;
                if(totalguest_secondtype-min>0) System.out.println("NO");
                else if(totalguest_secondtype-min==0) System.out.println("YES");
                else
                {
                    long left=min- totalguest_secondtype;
                     if(left+(vanilla+choclate)-min>=totalguest_firsttype) System.out.println("YES");
                    else System.out.println("NO");
                }
            }
        }
    }
}
